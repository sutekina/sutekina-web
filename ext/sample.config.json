{   
    "README": "Before configuring this you might wanna look into the config documentations unless you know what you are doing. https://github.com/sutekina/sutekina-web/blob/main/README.md#config-file",
    "app_name": "Sutekina",
    "port": 4000,
    "gulag_path": "/home/osu/gulag",
    "debug": {
        "logging": {
            "terminal_level": "all",
            "file_level": "all",
            "timestamp_format": "DD-MM-YYYY HH:mm:ss.SSS"
        },
        "install_modules_on_boot": true
    },
    "middleware": {
        "use_cors": false
    },
    "protocol": {
        "https": false,
        "cert": "",
        "key": ""
    },
    "cookies": {
        "name": "session",
        "secret": "secret"
    },
    "mysql": {
        "user": "user",
        "host": "localhost",
        "password": "password",
        "port": "3306",
        "database": "database",
        "timezone": "Z",
        "insecureAuth": false,
        "waitForConnections": true,
        "connectionLimit": 10,
        "queueLimit": 0,
        "multipleStatements": true
    },
    "domains": {
        "avatar": "a.sutekina.xyz",
        "base": "sutekina.xyz",
        "api": "api.sutekina.xyz",
        "discord": "https://discord.com/invite/xRCMXJ7JPh",
        "github:": "https://github.com/sutekina/"
    },
    "local": {
        "api": {
            "host": "localhost",
            "port": "4001"
        }
    },
    "authentication": {
        "osu": "OSU-API-TOKEN"
    },
    "settings": {
        "username": {
            "minimum": 2,
            "maximum": 15,
            "forbidden_names": ["#Guacamole2"],
            "allowed_characters": ["\\w", "\\-", " ", "\\[\\]"]
        },
        "password": {
            "minimum": 8,
            "maximum": 32,
            "forbidden_characters": []
        }
    }
}